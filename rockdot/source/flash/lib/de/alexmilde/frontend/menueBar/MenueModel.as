package de.alexmilde.frontend.menueBar {	import flash.display.Sprite;	/**	 * @author alexmilde	 */	public class MenueModel 	{		// dimension of the bar		private var _dimension : int;		// current dimension of the bar		private var _dimensionCurrent : int;		// background image with repeat-x		private var _background : Sprite = new Sprite();		// array with sprite elements		private var _elements : Array = new Array();								public function get background() : Sprite		{			return _background;		}						public function set background(background : Sprite) : void		{			_background = background;		}						public function get elements() : Array		{			return _elements;		}						public function set elements(elements : Array) : void		{			_elements = elements;		}						public function get dimension() : int		{			return _dimension;		}						public function set dimension(dimension : int) : void		{			_dimension = dimension;		}						public function get dimensionCurrent() : int		{			return _dimensionCurrent;		}						public function set dimensionCurrent(dimensionCurrent : int) : void		{			_dimensionCurrent = dimensionCurrent;		}	}}