package de.alexmilde.frontend.menueBar {	import flash.display.Sprite;	import flash.events.MouseEvent;	import flash.geom.Point;	/**	 * @author alexmilde	 */	public class MenueElement extends Sprite	{		private var _pos : Point = new Point();		private var _posType : String;		private var _dispatchStr : String;		private var _activeVersion : Boolean;		private var _stateBehaviour : String;				private var _isActive : Boolean = false;		private var _startActive : Boolean;		public function MenueElement(stdImg : Sprite, pos : Point, posType : String, activeVersion : Boolean, dispatches : String, stateBehaviour : String, startActive : Boolean)		{			this.addChild(stdImg);			_pos = pos;			_posType = posType;			_activeVersion = activeVersion;			_dispatchStr = dispatches;			_stateBehaviour = stateBehaviour;			_startActive = startActive;		}				public function get posType() : String		{			return _posType;		}						public function get activeVersion() : Boolean		{			return _activeVersion;		}						public function get dispatchStr() : String		{			return _dispatchStr;		}				public function dispatchClick(event : MouseEvent) : void		{			dispatchEvent(new SimpleMenueEvent(_dispatchStr, true));		}		public function dispatchRollOver(event : MouseEvent) : void		{			dispatchEvent(new SimpleMenueEvent(_dispatchStr + ElementState.ROLLOVER, false));		}				public function dispatchRollOut(event : MouseEvent) : void		{			dispatchEvent(new SimpleMenueEvent(_dispatchStr + ElementState.ROLLOUT, false));		}						public function get stateBehaviour() : String		{			return _stateBehaviour;		}						public function get isActive() : Boolean		{			return _isActive;		}						public function set isActive(isActive : Boolean) : void		{			_isActive = isActive;		}						public function get pos() : Point		{			return _pos;		}						public function get startActive() : Boolean		{			return _startActive;		}			}}