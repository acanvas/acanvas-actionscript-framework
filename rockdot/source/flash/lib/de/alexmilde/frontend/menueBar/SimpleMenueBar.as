package de.alexmilde.frontend.menueBar {	import flash.display.Sprite;	import flash.geom.Point;	/**	 * @author alexmilde	 */	public class SimpleMenueBar extends Sprite 	{			private var _model : MenueModel = new MenueModel();		private var _controller : MenueController = new MenueController(_model);						public function SimpleMenueBar(dimension : int, background : Sprite)		{			_controller.initMenue(dimension, background);		}				public function addElement(stdImg : Sprite, 									pos : Point, 									posType : String = ElementState.POSLEFT,									activeVersion : Boolean = ElementState.INACTIVE,									dispatches : String = null, stateBehaviour : String = null,									startActive : Boolean = false									) : void		{			_controller.addElement(new MenueElement(stdImg, 													pos, 													posType, 													activeVersion, 													dispatches, 													stateBehaviour,													startActive));		}				public function create() : void		{			this.addChild(_model.background);						// fill graphic holder			for (var i : int = 0; i < _model.elements.length; i++) 			{				this.addChild(MenueElement(_model.elements[i]));			}							_controller.setBarElements();		}						public function updateView(stageWidth : int) : void		{			_controller.updateMenue(stageWidth);		}						public function get model() : MenueModel		{			return _model;		}						public function get controller() : MenueController		{			return _controller;		}	}}