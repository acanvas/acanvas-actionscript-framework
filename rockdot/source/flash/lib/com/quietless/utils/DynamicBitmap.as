package com.quietless.utils {	import flash.display.BitmapData;	import flash.display.DisplayObject;	import flash.geom.Matrix;	public class DynamicBitmap extends DynamicBitmapData {				public function DynamicBitmap(targ:DisplayObject, sname:String, quality : Number = 80) {			var bmd : BitmapData = new BitmapData(targ.width, targ.height, true, 0xFFFFFF);			bmd.draw(targ, new Matrix(), null, null, null, false);		    			super(bmd, sname, quality);		}						}	}