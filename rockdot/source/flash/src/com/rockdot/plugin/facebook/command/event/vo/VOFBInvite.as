package com.rockdot.plugin.facebook.command.event.vo {	public class VOFBInvite	{		//http://developers.facebook.com/docs/reference/dialogs/requests/				public var app_id : String;		public var method : String;		public var display : String;		public var redirect_uri : String;		public var message : String;		public var to : String;		public var filters : String;		public var exclude_ids : String;		public var data : String;		public var title : String;		public var max_recipients : int;		//Invite Source, will be put into 'data' String		public var reason : String;		public function VOFBInvite(title : String = "", message : String = "", data : String = "", reason : String = "", to : String = "", max_recipients : int = 0) {			this.title = title;			this.data = data;			this.message = message;			this.reason = reason;			this.to = to;						if(max_recipients>0){				this.max_recipients = max_recipients;			}		}			}}