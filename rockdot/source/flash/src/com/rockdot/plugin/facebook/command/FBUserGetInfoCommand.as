package com.rockdot.plugin.facebook.command {	import com.rockdot.core.mvc.RockdotEvent;	import com.rockdot.plugin.facebook.model.FacebookConstants;	import com.rockdot.plugin.facebook.model.vo.FBUserVO;	import org.as3commons.async.operation.IOperation;	import org.as3commons.async.operation.event.OperationEvent;	public class FBUserGetInfoCommand extends AbstractFBCommand {		override public function execute(event : RockdotEvent = null) : * {			super.execute(event);//			dispatchMessage("loading.facebook.login");						var operation : IOperation = _context.getObject(FacebookConstants.OPERATION_FB, ["fql.query", {query:"SELECT uid, name, pic_square, is_app_user, birthday_date, email, hometown_location, locale FROM user WHERE uid = me()"}]); //trick!			operation.addCompleteListener(_handleComplete);			operation.addErrorListener(_handleError);		}
		private function _handleComplete(event : OperationEvent) : void {			var user : FBUserVO = new FBUserVO(event.result[0]);			if (!user) {				error = ( "Error parsing user from " + event.result );				dispatchErrorEvent();				return;			}			_fbModel.user = user;			if(event.result[0].hometown_location is Object){				_fbModel.user.hometown_location = event.result[0].hometown_location.name;			}			_fbModel.user.id = user.uid;			dispatchCompleteEvent(_fbModel.user);		} 
	}}